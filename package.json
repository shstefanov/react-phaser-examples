{
    "author": "",
    "dependencies": {
        "@orbits/react-phaser": "git+https://github.com/shstefanov/react-phaser.git#development",
        "combine-section-reducers": "^1.0.6",
        "npm-run-all": "^4.1.5",
        "phaser": "^3.86.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.26.2",
        "redux": "^5.0.1"
    },
    "description": "React starter with esbuild",
    "devDependencies": {
        "esbuild": "^0.23.1",
        "mocha": "^10.7.3",
        "servor": "^4.0.2"
    },
    "keywords": [],
    "license": "ISC",
    "main": "index.js",
    "name": "react-helloworld",
    "scripts": {
        "build": "esbuild --preserve-symlinks ./src/index.entry.css ./src/index.entry.js ./src/index.html --loader:.svg=dataurl --loader:.png=dataurl --loader:.jpg=dataurl --loader:.cur=dataurl --loader:.ani=dataurl --loader:.ttf=dataurl --loader:.html=copy --minify --bundle --outdir=./public",
        "build:dev": "esbuild --preserve-symlinks ./src/index.entry.css ./src/index.entry.jsx ./src/index.html --loader:.svg=dataurl --loader:.png=dataurl --loader:.jpg=dataurl --loader:.cur=dataurl --loader:.ani=dataurl --loader:.ttf=dataurl --loader:.html=copy --watch  --bundle --sourcemap --outdir=./public",
        "servor": "servor ./public index.html 3100",
        "start": "npm-run-all --parallel servor build:dev",
        "test": "mocha -b -p --recursive --bail --extension spec.js -- "
    },
    "version": "1.0.0"
}
