{
    "author": "",
    "dependencies": {
        "@orbits/react-phaser": "git+https://github.com/shstefanov/react-phaser.git#main",
        "combine-section-reducers": "^1.0.6",
        "phaser": "^3.85.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.26.2",
        "redux": "^5.0.1"
    },
    "description": "React starter with esbuild",
    "devDependencies": {
        "esbuild": "^0.23.1",
        "mocha": "^10.7.3",
        "servor": "^4.0.2"
    },
    "keywords": [],
    "license": "ISC",
    "main": "index.js",
    "name": "react-helloworld",
    "scripts": {
        "build": "esbuild --preserve-symlinks $(find ./src -name *.entry.*) $(find ./src -name *.html) $(npm run esbuild-loaders | tail -n 1) --minify --bundle --outdir=./public",
        "esbuild-loaders": "export L=--loader:; export D=dataurl; echo $L.svg=$D $L.png=$D $L.jpg=$D $L.cur=$D $L.ani=$D $L.ttf=$D $L.html=copy",
        "links": "npm link $(cat links.txt)",
        "prestart": "npm run show-entries",
        "servor": "servor ./public index.html 3100",
        "show-entries": "echo Entrypoints: && find ./src -name *.entry.*",
        "start": "esbuild --preserve-symlinks $(find ./src -name *.entry.*) $(find ./src -name *.html) $(npm run esbuild-loaders | tail -n 1) --watch  --bundle --sourcemap --outdir=./public | npm run servor",
        "test": "mocha -b -p --recursive --bail --extension spec.js -- "
    },
    "version": "1.0.0"
}
